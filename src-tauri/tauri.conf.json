{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "sd-script-app",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "active-venv",
            "cmd": ".\\venv\\Scripts\\activate",
            "args": []
          },
          {
            "name": "create-venv",
            "cmd": "python",
            "args": ["-m", "venv", "venv"]
          },
          {
            "name": "pwd",
            "cmd": "pwd",
            "args": []
          },
          {
            "name": "train",
            "cmd": "python -m accelerate.commands.launch --num_cpu_threads_per_process=8 train_network.py",
            "args": []
          }
        ]
      },
      "path": {
        "all": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.kurilee.sdscriptapp",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "sd-script-app",
        "width": 800,
        "height": 600
      }
    ]
  }
}
